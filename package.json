{
  "name": "react2angular",
  "version": "5.0.0",
  "description": "The easiest way to embed React components in Angular 1 apps!",
  "main": "dist/main.js",
  "typings": "dist/main.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean && yarn lint && tsc -p tsconfig.build.json",
    "clean": "rimraf dist",
    "lint": "tslint --fix -p ./tsconfig.json 'src/**/*'",
    "pretest": "yarn build && tsc -p tsconfig.test.json",
    "prepublishOnly": "yarn test",
    "test": "karma start --single-run",
    "test:watch": "karma start"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/coatue-oss/react2angular.git"
  },
  "keywords": [
    "react",
    "angular",
    "interop",
    "ngreact",
    "combine",
    "together",
    "embed"
  ],
  "author": "Boris Cherny <boris@performancejs.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/coatue-oss/react2angular/issues"
  },
  "homepage": "https://github.com/coatue-oss/react2angular#readme",
  "peerDependencies": {
    "@types/angular": ">=1.8",
    "@types/prop-types": ">=15",
    "@types/react": ">=18",
    "@types/react-dom": ">=18",
    "prop-types": ">=15",
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@d3banking/prettier-config": "^2.1.8",
    "@types/angular": "^1.8.5",
    "@types/angular-mocks": "^1.7.1",
    "@types/jasmine": "^3.3.9",
    "@types/jquery": "^3.3.29",
    "@types/prop-types": "^15.7.0",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "angular-mocks": "1.8.3",
    "angular-resource": "^1.8.3",
    "browserify": "^17.0.0",
    "jasmine": "^4.6.0",
    "jasmine-core": "^4.6.0",
    "karma": "^6.4.2",
    "karma-browserify": "^8.1.0",
    "karma-chrome-launcher": "^3.2.0",
    "karma-jasmine": "^5.1.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-source-map-support": "^1.4.0",
    "ngimport": "^1.0.0",
    "prettier": "^2.8.8",
    "prop-types": "^15.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.0",
    "rollupify": "^0.5.1",
    "tslint": "^5.13.1",
    "typescript": "^5.0.4",
    "watchify": "^4.0.0",
    "webpack": "^5.82.1"
  },
  "dependencies": {
    "@types/lodash.frompairs": "^4.0.5",
    "angular": ">=1.5",
    "lodash.frompairs": "^4.0.1",
    "ngcomponent": "^4.1.0"
  },
  "prettier": "@d3banking/prettier-config"
}
